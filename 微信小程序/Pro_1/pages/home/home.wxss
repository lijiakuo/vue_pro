/* pages/home/home.wxss */
page{
  width: 100%;
  height: 100%;
  background: #F9F8FE; 
}
.sw-box{
  display: block;
  width: 100%;
  height: 150px;
  position: relative;
}
.sw{
  box-sizing: border-box;
   padding: 0 10px; 
}
.sw-box image{
  width: 100%;
  height: 100%;
}
.sw-box swiper{
  width: 100%;
  height: 100%;
}
.bgs{
  width: 100%;
  height: 100%;
  position: absolute;
  top:0;
  left:0;
  z-index: 100;
}
.xx{
  width: 24px;
  height: 24px;
}
.lists{
  padding: 0 10px;
}
.list{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  border: 1px solid #fff;
  border-radius: 10px;
}
.list .list-item{
  width: 23%;
  height: 60px;
  text-align: center;
  margin-top: 20px;
}
.list-item image {
  width: 30px;
  height: 30px;
}
